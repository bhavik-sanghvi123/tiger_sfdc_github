    <decisions>
        <name>Check_Status_based_on_Lead_Scoring_Setting</name>
        <label>Check Status based on Lead Scoring Setting</label>
        <locationX>660</locationX>
        <locationY>575</locationY>
        <defaultConnector>
            <targetReference>Loop_Into_Lead_Scoring_Setting</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>Default Outcome</defaultConnectorLabel>
        <rules>
            <name>MY_Cold</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>$Record.Total_Lead_Score__c</leftValueReference>
                <operator>LessThanOrEqualTo</operator>
                <rightValue>
                    <elementReference>Loop_Into_Lead_Scoring_Setting.Range_Max__c</elementReference>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>Loop_Into_Lead_Scoring_Setting.Market__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>MY</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>Loop_Into_Lead_Scoring_Setting.Range_Min__c</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Update_Lead_Status_Cold</targetReference>
            </connector>
            <label>MY Cold</label>
        </rules>
        <rules>
            <name>MY_Warm</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>$Record.Total_Lead_Score__c</leftValueReference>
                <operator>GreaterThanOrEqualTo</operator>
                <rightValue>
                    <elementReference>Loop_Into_Lead_Scoring_Setting.Range_Min__c</elementReference>
                </rightValue>
            </conditions>
            <conditions>
