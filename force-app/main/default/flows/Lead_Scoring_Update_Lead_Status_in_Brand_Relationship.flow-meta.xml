                <operator>LessThanOrEqualTo</operator>
                <rightValue>
                    <elementReference>Loop_Into_Lead_Scoring_Setting.Range_Max__c</elementReference>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>Loop_Into_Lead_Scoring_Setting.Market__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>MY</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Update_Lead_Status_Warm</targetReference>
            </connector>
            <label>MY Warm</label>
        </rules>
        <rules>
            <name>MY_Hot</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>$Record.Total_Lead_Score__c</leftValueReference>
                <operator>GreaterThanOrEqualTo</operator>
                <rightValue>
                    <elementReference>Loop_Into_Lead_Scoring_Setting.Range_Min__c</elementReference>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>Loop_Into_Lead_Scoring_Setting.Market__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>MY</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>Loop_Into_Lead_Scoring_Setting.Range_Max__c</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Update_Lead_Status_Hot</targetReference>
            </connector>
            <label>MY Hot</label>
        </rules>
