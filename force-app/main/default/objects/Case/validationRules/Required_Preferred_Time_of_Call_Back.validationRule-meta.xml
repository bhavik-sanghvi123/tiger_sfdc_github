<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Required_Preferred_Time_of_Call_Back</fullName>
    <active>true</active>
    <errorConditionFormula>AND(FollowUpCallRequired__c = TRUE,ISBLANK(TEXT( PreferredDateTimeofCallback__c )) 
)</errorConditionFormula>
    <errorMessage>Preferred Date &amp; Time of Call back is required.</errorMessage>
</ValidationRule>
